{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "no-restricted-globals": 0,
        "no-undef": 0,
        "consistent-return": 0,
        "no-unused-vars": 0
    }
}
